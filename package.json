        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.2.15",
        "postcss-import": "^14.0.2",
        "resolve-url-loader": "^4.0.0",
        "tailwindcss": "^2.1.2",
        "webpack": "^5.37.0",
        "webpack-cli": "^4.7.0"
    }
